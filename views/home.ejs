<!DOCTYPE html>
<html>
  <head>
    <title>Basic Web Page</title>
    <!-- Latest compiled and minified CSS -->
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css"
      rel="stylesheet"
    />
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js"></script>
    <script src="https://code.jquery.com/jquery-3.5.1.js"></script>
    <script src="/static/js/appMain.js"></script>
  </head>
  <body>
    <div class="m-4" id="file_tab">
      <ul class="nav nav-tabs">
        <li class="nav-item">
          <a href="#home" class="nav-link active">Home</a>
        </li>
        <li class="nav-item">
          <a href="#profile" class="nav-link">Profile</a>
        </li>
        <li class="nav-item">
          <a href="#messages" class="nav-link">Messages</a>
        </li>
      </ul>
      <div class="tab-content">
        <div>
          <label for="fileName">Enter the fileName:</label
          ><input id="fileName" type="text" />
          <button type="button" id="searchBtn" class="primary">Search</button>
        </div>
        <div class="tab-pane fade show active" id="home">
          <table id="fileTable" class="invisible table table-bordered">
            <thead>
              <tr>
                <th>File Name</th>
                <th>Modified date</th>
              </tr>
            </thead>
            <tbody></tbody>
          </table>
        </div>
        <div class="tab-pane fade" id="profile">
          <p>Profile tab content ...</p>
        </div>
        <div class="tab-pane fade" id="messages">
          <p>Messages tab content ...</p>
        </div>
      </div>
    </div>
  </body>
</html>
